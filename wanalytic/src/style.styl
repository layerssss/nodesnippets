html,body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td,em,button{margin:0;padding:0;}
body{ font:12px/1.5 arial,Simsun, sans-serif; color:#333; background-color: #fff; }
img, fieldset{border:none;vertical-align:middle;}
input, textarea{font-size:12px;}
table{border-collapse:collapse;}
a{color:#06c;text-decoration:none;}
a:hover{text-decoration:none;}
h1,h2,h3,h4,h5{font-family:"Times New Roman",Simsun;}
ul,ol{list-style:none;}
input[type="button"],input[type="submit"]{-webkit-appearance:none;}
input[type="text"]{border-radius:0;}
h2{font-size: 18px;}
h3{font-size:16px;}

.top{ width: 100%; height: 40px; background-color: #155ebd; position: fixed; left: 0; top: 0;}
.home,.ef,.nav{ float: left;}
.qb{ float: right;}
.nb li{ float: left; border-left:#6696d4 solid 1px;}
.nb li a{ display: inline-block; line-height: 40px; padding: 0 20px; color: #fff;text-shadow:#1355ac 0 -1px;}
.nb li a:hover{ background-image:-webkit-gradient(linear,left bottom,left top,from(#1355ac),to(#083672));}
.home{ width:50px;height:40px; margin-right:10px;}
.home a{width: 50px; height: 40px; display: inline-block; background:url(../images/home_fff.png) no-repeat center center;}
.ef{ padding-right:15px; position:relative;border-left:#6696d4 solid 1px; z-index: 9999;}
.app_icon{ float:left; background:url(../images/globe-white.png) no-repeat center center; width:40px; height: 40px; }
.action_open{ float:left; width:320px;margin-top:2px; color:#fff;cursor:pointer;}
.x1:before,.x1:after,.se:before,.se:after,.action_open:before,.action_open:after,.buttonText:before,.buttonText:after{content:"";position:absolute;top:50%;left:0;}
.x1:before,.se:before,.action_open:before,.buttonText:before{
	left:350px;
	top:25px;
    border: 8px solid transparent;
    border-width:4px;
    border-top-color: #fff;
    background:transparent;
}
.se:before{left:2px;top:6px; border-left-color: #333;}
.x1:before{left:2px;top:6px; border-left-color: #333;}
.account_2{ font-weight: bold;}
.account_list{ position:absolute; top:40px; left:-60px; -webkit-box-shadow:0 3px 10px rgba(0,0,0,.3); border:1px solid #ccc; background-color:#fff; }
.I5{ padding:6px 8px; background-color:#f2f2f2; border-bottom:1px solid #ccc; overflow: hidden;}
.floatRight{ float:right;}
.action_gohome,.buttonText{ float:left; cursor:pointer; padding:8px 7px; text-align:center; border-radius:3px; border:solid 1px #ccc; background-color:#f3f3f3; background-image:-webkit-linear-gradient(top,#fefefe,#f3f3f3);}
.TO{ padding-left:5px;}
.account_m{ margin:0 10px 30px 0; width:510px;}
.action_collapse{ margin-top:15px; overflow:hidden;position:relative;}
.se{ float:left; margin-right:10px; padding-left:15px; width:390px;}
.jxb{ color:#aaa; float:right;}
.afb{ color:#aaa;}
input[type='text']{ background:url(../images/search_30.png) no-repeat 0 center; padding:5px 5px 5px 25px;-webkit-box-shadow:inset 0 1px 0 #d6d6d6;}
.main{ margin-top:40px; overflow: hidden;}
.sidebar{ width: 233px; height:auto; float:left; background-color: #f9f9f9; border-right:1px solid #c8c8c8;  margin-bottom:-9999px; padding-bottom:9999px;}
#ID-searchPanel{margin:20px 11px 18px 15px}
.ar{ background-image: -webkit-gradient(linear,0 0,0 100%,from(#f9f9f9),to(#f2f2f2)); background-image: -moz-linear-gradient(center top,#f9f9f9,#f2f2f2); border-bottom: 1px solid #dedede;color: #666;
font-size: 12px; font-weight: bold; padding: 5px 5px 5px 15px; text-transform: uppercase;}
.hc{color: #444; font-size: 13px; font-weight: normal; padding: 0; cursor:pointer; text-transform: none;border-bottom:1px solid #ccc; background-image:-webkit-gradient(linear,0 0,0 100%,from(#f9f9f9),to(#f2f2f2));}
.hc .mb{ width: 30px; height: 40px; float:left;}
.hc .hm{ height: 40px; line-height: 40px;}
.ID-menu-item{ height:40px; line-height:40px;}
.IC{ padding:0 8px 2px 40px; border: none 0;}
.IC a{ color:#444; padding:6px 0 6px 8px; display: inline-block;}
.view{ width: auto; height:1000px; overflow: hidden; padding: 10px 10px 0 10px;}
.view_top{ margin-bottom: 5px;}
.view_top input{ float:right;}
.view_top h2{ display: inline-block;}
.tb{ width: 100%;}
.tb td, .tb th{ border:1px solid #ddd; text-align: center; padding:8px 0;}
.tb th{ background-color:#E9E9E7; cursor:pointer;}
.db {margin: 5px 0;}
.db span{ line-height: 40px; display: inline-block; float: left; margin:0 5px;}
.primary{ position:relative; overflow: hidden; height: 40px; display:inline-block; float: left; z-index: -1;}

